{
  "version": "1.0.1",
  "name": "mui-message",
  "description": "Send messages (snackBar messages) as convenient as using antd when use MUI(@mui/material), without using hooks or creating Snackbar components frequently.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/mui-message.esm.js",
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build --transpileOnly --entry ./src/index.tsx",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "keywords": [
    "mui",
    "material-ui",
    "antd",
    "snackbar",
    "notistack",
    "iimm",
    "www.iimm.ink",
    "liudichen",
    "notification",
    "react",
    "@mui/material",
    "material UI",
    "hooks",
    "useMessage",
    "MessageBox",
    "message"
  ],
  "author": {
    "name": "liudichen",
    "email": "liudichen@foxmail.com",
    "url": "https://www.iimm.ink"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "size-limit": [
    {
      "path": "dist/mui-message.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/mui-message.esm.js",
      "limit": "10 KB"
    }
  ],
  "peerDependencies": {
    "@emotion/react": "^11.0.0",
    "@emotion/styled": "^11.0.0",
    "@mui/icons-material": "^5.10.0",
    "@mui/material": "^5.10.0",
    "react": "^16.8.0 || ^17.0.2 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.2 || ^18.0.0"
  },
  "repository": {
    "url": "https://github.com/liudichen/mui-message",
    "type": "https"
  },
  "peerDependenciesMeta": {
    "@emotion/react": {
      "optional": true
    },
    "@emotion/styled": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-react": "^7.18.6",
    "@babel/runtime": "^7.20.1",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.10.14",
    "@mui/material": "^5.10.14",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/jest": "^29.2.3",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "^8.27.0",
    "eslint-config-egg": "^12.1.0",
    "eslint-plugin-react": "^7.31.10",
    "husky": "^7.0.4",
    "parcel": "^2.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "notistack": "^2.0.8"
  },
  "engines": {
    "node": ">=14"
  }
}
